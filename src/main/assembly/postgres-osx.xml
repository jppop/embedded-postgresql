<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>Darwin-x86_64</id>
	<formats>
		<format>tar.gz</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>${project.build.directory}/dist/postgresql-osx/pgsql</directory>
			<outputDirectory></outputDirectory>
			<fileMode>0777</fileMode>
			<directoryMode>0777</directoryMode>
			<filtered>false</filtered>
			<includes>
				<include>share/postgresql</include>
				<include>lib/libiconv.2.dylib</include>
				<include>lib/libxml2.2.dylib</include>
				<include>lib/libssl.1.0.0.dylib</include>
				<include>lib/libcrypto.1.0.0.dylib</include>
				<include>lib/libuuid.1.1.dylib</include>
				<include>lib/postgresql/*.so</include>
				<include>bin/initdb</include>
				<include>bin/pg_ctl</include>
				<include>bin/postgres</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>
