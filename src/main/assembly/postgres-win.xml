<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>Windows-x86_64</id>
	<formats>
		<format>tar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>${project.build.directory}/dist/postgresql-win/pgsql</directory>
			<outputDirectory></outputDirectory>
			<fileMode>0777</fileMode>
			<directoryMode>0777</directoryMode>
			<filtered>false</filtered>
			<includes>
				<include>share</include>
				<include>lib/iconv.lib</include>
				<include>lib/libxml2.lib</include>
				<include>lib/ssleay32.lib</include>
				<include>lib/ssleay32MD.lib</include>
				<include>lib/*.dll</include>
				<include>bin/initdb.exe</include>
				<include>bin/pg_ctl.exe</include>
				<include>bin/postgres.exe</include>
				<include>bin/*.dll</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>
